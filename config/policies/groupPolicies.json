{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Action": [
        "iam:ListRoles",
        "iam:GetRolePolicy",
        "iam:PassRole",
        "apigateway:*",
        "dynamodb:*",
        "logs:*",
        "cloudwatch:*",
        "lambda:*",
        "s3:*"
      ],
      "Resource": "*"
    }
  ]
}